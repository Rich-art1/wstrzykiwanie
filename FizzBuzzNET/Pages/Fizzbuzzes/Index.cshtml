@page
@model FizzBuzzNET.Pages.Fizzbuzzes.IndexModel

@{ ViewData["Title"] = "Index"; }


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Fizzbuzz[0].Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fizzbuzz[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fizzbuzz[0].Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fizzbuzz[0].Data)
            </th>
            <th></th>
        </tr>
    </thead>
 
    <tbody>
        @foreach (var item in Model.Fizzbuzz.Take(20))
        {
            {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Number)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Data)
    </td>
    <td>
        <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
    </td>
</tr>}
            }
        @foreach (var item in Model.Fizzbuzz.Skip(20))
        {
            {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.LastName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Number)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Data)
    </td>
    <td>
        <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
    </td>
</tr>}
            }
    </tbody>
</table>
